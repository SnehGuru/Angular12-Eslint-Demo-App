{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
       
        "indent": [
            "error",
           // "warn",
            // "off",
            2
        ],
        "eqeqeq": "error",
        "no-restricted-imports": ["error", "underscore"],
        "consistent-return": "error",
        "no-console": "error",
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
